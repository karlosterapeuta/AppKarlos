{
  "name": "musicoterapia-app",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=20.10.0"
  },
  "scripts": {
    "dev": "next dev",
    "setup": "node scripts/setup-sqlite.js",
    "init-db": "node scripts/init-db.js",
    "prebuild": "mkdir -p .next/sqlite && chmod -R 777 .next/sqlite && touch .next/sqlite/prisma.db && chmod 666 .next/sqlite/prisma.db && cp prisma/schema.netlify.prisma prisma/schema.prisma",
    "build": "prisma generate && prisma migrate reset --force && next build",
    "start": "next start -p $PORT",
    "lint": "next lint",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@auth/prisma-adapter": "2.7.4",
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.2.0",
    "@prisma/client": "^5.7.1",
    "bcryptjs": "^2.4.3",
    "next": "14.2.24",
    "next-auth": "^4.24.5",
    "prisma": "^5.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.12.0"
  },
  "devDependencies": {
    "@auth/prisma-adapter": "^2.7.4",
    "@radix-ui/react-checkbox": "^1.1.4",
    "@react-pdf/renderer": "^4.2.1",
    "@supabase/supabase-js": "^2.48.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20.10.6",
    "@types/react": "^18.2.46",
    "@types/react-dom": "^19.0.3",
    "autoprefixer": "^10.4.16",
    "class-variance-authority": "^0.7.1",
    "eslint": "^8.56.0",
    "eslint-config-next": "14.0.4",
    "html2canvas": "^1.4.1",
    "jspdf": "^2.5.2",
    "lucide-react": "^0.475.0",
    "moment": "^2.30.1",
    "next": "^14.2.24",
    "postcss": "^8.4.32",
    "postcss-preset-env": "^10.1.3",
    "react-big-calendar": "^1.17.1",
    "react-hot-toast": "^2.5.1",
    "recharts": "^2.15.1",
    "tailwind-merge": "^3.0.1",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.3.3"
  }
}
