{
  "name": "musicoterapia-app",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=20.10.0"
  },
  "scripts": {
    "dev": "next dev",
    "setup": "node scripts/setup-sqlite.js",
    "init-db": "node scripts/init-db.js",
    "build": "NODE_ENV=production NEXT_TELEMETRY_DISABLED=1 SKIP_TYPE_CHECK=true rm -f .next/sqlite/prisma.db && cp prisma/schema.netlify.prisma prisma/schema.prisma && prisma generate && prisma migrate reset --force && next build --no-lint",
    "start": "next start -p $PORT",
    "lint": "next lint",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^1.0.12",
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.2.0",
    "@prisma/client": "^5.7.1",
    "bcryptjs": "^2.4.3",
    "next": "14.0.4",
    "next-auth": "^4.24.5",
    "prisma": "^5.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.12.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20.10.6",
    "@types/react": "^18.2.46",
    "@types/react-dom": "^19.0.3",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.56.0",
    "eslint-config-next": "14.0.4",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.3.3"
  }
}
